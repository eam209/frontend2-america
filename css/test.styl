@import 'nib/vendor'

@import 'nib/gradients'

vendor-prefixes = o ms moz webkit official

body
  	background linear-gradient(top, white, skyblue)
  	max-width 1000px
  	margin auto
	
	 .contenedor
		 height 0
		 overflow hidden
		 padding-bottom 56.25%
		 padding-top 30px
		 position relative
		 iframe
		 	height 100%
		 	left 0
		 	position absolute
		 	top 0
		 	width 100%
		 	
		
		 	
